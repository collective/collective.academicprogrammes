<model xmlns:security="http://namespaces.plone.org/supermodel/security" xmlns:marshal="http://namespaces.plone.org/supermodel/marshal" xmlns:form="http://namespaces.plone.org/supermodel/form" xmlns="http://namespaces.plone.org/supermodel/schema">
  <schema>
    <field name="course_name" type="zope.schema.TextLine">
      <description>course name</description>
      <title>course name</title>
    </field>
    <field name="course_code" type="zope.schema.TextLine">
      <description>course code</description>
      <title>course code</title>
    </field>
    <field name="course_level" type="zope.schema.Choice">
      <description>Which year (1,2 or 3)</description>
      <title>course level</title>
      <values>
        <element>1</element>
        <element>2</element>
        <element>3</element>
      </values>
    </field>
    <field name="semester" type="zope.schema.Set">
      <description>Which semester</description>
      <title>semester</title>
      <value_type type="zope.schema.Choice">
        <values>
          <element>1</element>
          <element>2</element>
          <element>3</element>
        </values>
      </value_type>
    </field>
    <field name="credits" type="zope.schema.Choice">
      <description/>
      <title>Credits</title>
      <values>
        <element>1</element>
        <element>2</element>
        <element>3</element>
        <element>4</element>
        <element>5</element>
        <element>6</element>
        <element>7</element>
        <element>8</element>
        <element>9</element>
      </values>
    </field>
  </schema>
</model>